/* calclue the number of minutes in a day*/
 
 import java.util.*;
 
 public class Main{
 public static void main (String[] args) {
     int ans=0;
      Scanner sc= new Scanner(System.in);
      
      System.out.println("enter the day");
      int n= sc.nextInt();
      ans=24*n*60;
      System.out.println(ans);
      
      
 }
 }
 
